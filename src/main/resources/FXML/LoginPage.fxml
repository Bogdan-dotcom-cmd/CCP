<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Login.Login">
   <children>
      <TextField id="username" fx:id="username" layoutX="227.0" layoutY="127.0" promptText="username" />
      <TextField id="password" fx:id="Password" layoutX="227.0" layoutY="166.0" promptText="password" />
      <CheckBox fx:id="ShowPassword" layoutX="227.0" layoutY="200.0" mnemonicParsing="false" onAction="#ShowAndHide" text="Hide/Show Password" />
      <PasswordField fx:id="pass_hid" layoutX="227.0" layoutY="166.0" promptText="password" />
      <Hyperlink id="register" fx:id="GoToRegister" layoutX="198.0" layoutY="280.0" onAction="#HyperLinkConnection" text="Don't Have an account yet? Click here " />
      <Button id="login" fx:id="Login" layoutX="278.0" layoutY="235.0" mnemonicParsing="false" onAction="#Login" text="Login" />
   </children>

</AnchorPane>


